{% extends "@FOSUser/layout.html.twig" %}
{% block stylesheets %}
    {{ parent() }}
<!--side menu plugin-->
<link href="{{ asset('bundles/DPCUser/plugins/hoe-nav/hoe.css') }}" rel="stylesheet">
<!-- icons-->
<link href="{{ asset('bundles/DPCUser/plugins/ionicons/css/ionicons.min.css') }}" rel="stylesheet">
<link href="{{ asset('bundles/DPCUser/plugins/iCheck/custom.css') }}" rel="stylesheet">
<!--template custom css file-->
<link href="{{ asset('bundles/DPCUser/css/style.css') }}" rel="stylesheet">
{% endblock %}
{% block fos_user_content %}
    {{ include('@FOSUser/Security/login_content.html.twig') }}
{% endblock fos_user_content %}

{% block javascript %}
    {{ parent() }}
<script src="{{ asset('bundles/DPCUser/plugins/hoe-nav/hoe.js') }}"></script>
<script src="{{ asset('bundles/DPCUser/plugins/slimscroll/jquery.slimscroll.min.js') }}"></script>
<script src="{{ asset('bundles/DPCUser/js/app.js') }}"></script>
<!-- iCheck -->
<script src="{{ asset('bundles/DPCUser/plugins/iCheck/icheck.min.js') }}"></script>
<script>
    $(document).ready(function(){
        $('.i-checks').iCheck({
            checkboxClass: 'icheckbox_flat-blue',
            radioClass: 'icheckbox_flat-blue'
        });
    });
</script>
{% endblock %}
